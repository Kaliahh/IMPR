# include <stdio.h>
# include <stdlib.h>
# include <string.h>


int write(void) {

}

int read(void) {

}



int main(void) {

  char choice[1];

  printf("Do you want to read or write? (r/w): ");
  scanf("%s", choice);

  if (strcmp(choice, r) == 0)
    read();
  else if (strcmp(choice, w) == 0)
    write();
  else
    printf("Invalid input.\n");
  
}
